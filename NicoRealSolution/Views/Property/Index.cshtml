                <!-- Explore Apartment -->
                <section class="pt0 pb60 pb30-md bgc-white" style="background: #FDFFEB">
                    <div class="container">
                        <div class="row align-items-center wow fadeInUp" data-wow-delay="00ms">
                            <div class="col-lg-9">
                                <div class="main-title2">
                                    <h2 class="title">Descopera Proprietatile Recomandate</h2>
                                    <!-- <p class="paragraph">Aliquam lacinia diam quis lacus euismod</p> -->
                                </div>
                            </div>
                            <div class="col-lg-3">
                                <div class="text-start text-lg-end mb-3">
                                    <a class="ud-btn2" asp-action="Listings">Vezi toate proprietățile<i class="fal fa-arrow-right-long"></i></a>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 wow fadeInUp" data-wow-delay="300ms">
                                <div class="feature-listing-slider2 vam_nav_style dots_none slider-dib-sm slider-3-grid owl-carousel owl-theme">
                                    @foreach (var prop in Model)
                                    {
                                        <div class="item">
                                            <a asp-action="Details" asp-controller="Property" asp-route-id="@prop.Id">
                                                <div class="listing-style1 style4" style="background: #FDFFEB">
                                                    <div class="list-thumb">
                                                        @if (!string.IsNullOrEmpty(prop.PhotoGuids))
                                                        {
                                                            var photoNames = prop.PhotoGuids.Split(new[] { ',' }, StringSplitOptions.RemoveEmptyEntries);
                                                            var firstPhoto = photoNames.FirstOrDefault();

                                                            <img class="w-100" src="@Url.Action("GetImage", "Property", new { key = firstPhoto })" alt="">
                                                        }
                                                        else
                                                        {
                                                            <img class="w-100" src="~/images/listings/g1-1.jpg" alt="">

                                                        }
                                                        <div class="list-tag fz12"><span class="flaticon-electricity me-2"></span>RECOMANDAT</div>

                                                        <div class="list-price">@prop.Price €</div>
                                                    </div>
                                                    <div class="list-content">
                                                        <h6 class="list-title"><a href="@Url.Action("Details", "Property", new { id = prop.Id })">@prop.Title</a></h6>
                                                        <p class="list-text">@prop.City , @prop.Country</p>
                                                        <div class="list-meta d-flex align-items-center">
                                                            @if (isMobileDevice)
                                                            {
                                                                <a href="@Url.Action("Details", "Property", new { id = prop.Id })"><span class="flaticon-home"></span>@prop.Category</a>
                                                                @if (prop.Surface != null)
                                                                {
                                                                    <a href="@Url.Action("Details", "Property", new { id = prop.Id })"><span class="flaticon-expand"></span>@prop.Surface m²</a>
                                                                }
                                                                @if (prop.Bathrooms != null)
                                                                {
                                                                    <a href="@Url.Action("Details", "Property", new { id = prop.Id })"><span class="flaticon-bed"></span>@prop.Rooms </a>
                                                                }

                                                                @if (prop.Bathrooms != null)
                                                                {

                                                                    <a href="@Url.Action("Details", "Property", new { id = prop.Id })"><span class="flaticon-shower"></span>@prop.Bathrooms </a>


                                                                }
                                                            }
                                                            else
                                                            {
                                                                <a href="@Url.Action("Details", "Property", new { id = prop.Id })"><span class="flaticon-home"></span>@prop.Category</a>
                                                                @if (prop.Surface != null)
                                                                {
                                                                    <a href="@Url.Action("Details", "Property", new { id = prop.Id })"><span class="flaticon-expand"></span>@prop.Surface m²</a>
                                                                }
                                                                @if (prop.Bathrooms != null)
                                                                {
                                                                    <a href="@Url.Action("Details", "Property", new { id = prop.Id })"><span class="flaticon-bed"></span>@prop.Rooms camere</a>
                                                                }

                                                                @if (prop.Bathrooms != null)
                                                                {
                                                                    if (prop.Bathrooms == 1)
                                                                    {
                                                                        <a href="@Url.Action("Details", "Property", new { id = prop.Id })"><span class="flaticon-shower"></span>@prop.Bathrooms baie</a>
                                                                    }
                                                                    else
                                                                    {
                                                                        <a href="@Url.Action("Details", "Property", new { id = prop.Id })"><span class="flaticon-shower"></span>@prop.Bathrooms băi</a>
                                                                    }
                                                                }
                                                            }
                                                        </div>
                                                        <hr class="mt-2 mb-2">
                                                    </div>
                                                </div>
                                            </a>
                                        </div>
                                    }

                                </div>
                            </div>
                        </div>
