                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>

                <!-- Explore Apartment -->
                <section class="pb90 pb30-md customSection">
                    <div class="container">
                        <div class="row justify-content-center wow fadeInUp" data-wow-delay="300ms">
                            <div class="col-lg-12">
                                <div class="dots_none slider-dib-sm slider-5-grid vam_nav_style owl-theme owl-carousel">
                                    <div class="item landingCarousel">
                                        <a asp-controller="Property" asp-action="Listings" asp-route-category="Casa/Vila">
                                            <div class="iconbox-style4">
                                                <span class="icon flaticon-home"></span>
                                                <div class="iconbox-content">
                                                    <h6 class="title">Case/Vile</h6>
                                                    @if (Convert.ToInt32(ViewData["Houses"]) == 1)
                                                    {
                                                        <p class="text mb-0">1 Anunț</p>
                                                    }
                                                    else
                                                    {
                                                        <p class="text mb-0">@ViewData["Houses"] Anunțuri</p>
                                                    }
                                                </div>
                                            </div>
                                        </a>
                                    </div>
                                    <div class="item landingCarousel">
                                        <a asp-controller="Property" asp-action="Listings" asp-route-category="Apartament">
                                            <div class="iconbox-style4">
                                                <span class="icon flaticon-corporation"></span>
                                                <div class="iconbox-content">
                                                    <h6 class="title">Apartamente</h6>
                                                    @if (Convert.ToInt32(ViewData["Apartments"]) == 1)
                                                    {
                                                        <p class="text mb-0">1 Anunț</p>
                                                    }
                                                    else
                                                    {
                                                        <p class="text mb-0">@ViewData["Apartments"] Anunțuri</p>
                                                    }
                                                </div>
                                            </div>
                                        </a>
                                    </div>
                                    <div class="item landingCarousel">
                                        <a asp-controller="Property" asp-action="Listings" asp-route-category="Teren">
                                            <div class="iconbox-style4">
                                                <span class="icon flaticon-garden"></span>
                                                <div class="iconbox-content">
                                                    <h6 class="title">Terenuri</h6>
                                                    @if (Convert.ToInt32(ViewData["Lands"]) == 1)
                                                    {
                                                        <p class="text mb-0">1 Anunț</p>
                                                    }
                                                    else
                                                    {
                                                        <p class="text mb-0">@ViewData["Lands"] Anunțuri</p>
                                                    }
                                                </div>
                                            </div>
                                        </a>
                                    </div>
                                    <div class="item landingCarousel">
                                        <a asp-controller="Property" asp-action="Listings" asp-route-category="Investiție">
                                            <div class="iconbox-style4">
                                                <span class="icon flaticon-chat"></span>
                                                <div class="iconbox-content">
