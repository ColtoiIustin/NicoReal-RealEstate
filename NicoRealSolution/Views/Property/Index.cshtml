                                    <div class="item landingCarousel">
                                        <a asp-controller="Property" asp-action="Listings" asp-route-category="Hotel">
                                            <div class="iconbox-style4">
                                                <span class="icon flaticon-network"></span>
                                                <div class="iconbox-content">
                                                    <h6 class="title">Hoteluri</h6>
                                                    @if (Convert.ToInt32(ViewData["Hotels"]) == 1)
