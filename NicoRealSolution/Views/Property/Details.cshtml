                            </div>
                            <div class="property-meta d-flex align-items-center">
                                <span style="color: green;"><i class="far fa-clock pe-2"></i>@Model.SingleProp.DatePosted</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4">
                        <div class="single-property-content">
                            <div class="property-action text-lg-end">
                                <div class="d-flex mb20 mb10-md align-items-center justify-content-lg-end">
                                </div>
                                <h3 class="price mb-0">@Model.SingleProp.Price €</h3>
                                @if (ViewBag.priceMp != null)
                                {
                                    <p class="text space fz15">@ViewBag.priceMp €/m²</p>
                                }
                                @if (Model.SingleProp.IsInvestment == "Da")
                                {
                                    <p class="text space fz15"><span style="color:green">Oportunitate Investiție</span></p>
                                }
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row wrap mt50 wow fadeInUp" data-wow-delay="500ms">
                    <div class="col-lg-8">
                        <div class="ps-widget mb30 overflow-hidden position-relative">
                            <h4 class="title fz17 mb30">General</h4>
                            <div class="row">
                                <div class="col-sm-6 col-lg-4">
                                    <div class="overview-element mb25 d-flex align-items-center">
                                        <span class="icon flaticon-home-1"></span>
                                        <div class="ml15">
                                            <h6 class="mb-0">Categorie</h6>
                                            <p class="text mb-0 fz15">@Model.SingleProp.Category</p>
                                        </div>
                                    </div>
                                </div>
                                @if (Model.SingleProp.Surface != null)
                                {
                                    <div class="col-sm-6 col-lg-4">
                                        <div class="overview-element mb25-xs d-flex align-items-center">
                                            <span class="icon flaticon-expand"></span>
                                            <div class="ml15">
                                                <h6 class="mb-0">Suprafață</h6>
                                                <p class="text mb-0 fz15">@Model.SingleProp.Surface m²</p>
                                            </div>
                                        </div>
                                    </div>
                                }
                                @if (Model.SingleProp.Rooms != null)
                                {
                                    <div class="col-sm-6 col-lg-4">
                                        <div class="overview-element mb25 d-flex align-items-center">
                                            <span class="icon flaticon-bed"></span>
                                            <div class="ml15">
                                                <h6 class="mb-0">Camere</h6>
                                                <p class="text mb-0 fz15">@Model.SingleProp.Rooms</p>
                                            </div>
                                        </div>
                                    </div>
                                }
                                @if (Model.SingleProp.Bathrooms != null)
                                {
                                    <div class="col-sm-6 col-lg-4">
                                        <div class="overview-element mb25 d-flex align-items-center">
                                            <span class="icon flaticon-shower"></span>
                                            <div class="ml15">
                                                <h6 class="mb-0">Băi</h6>
                                                <p class="text mb-0 fz15">@Model.SingleProp.Bathrooms</p>
                                            </div>
                                        </div>
                                    </div>
                                }
                                @if (Model.SingleProp.Floors != null)
                                {
                                    <div class="col-sm-6 col-lg-4">
                                        <div class="overview-element mb25-xs d-flex align-items-center">
                                            <span class="icon flaticon-garage"></span>
                                            <div class="ml15">
                                                <h6 class="mb-0">Etaje</h6>
                                                <p class="text mb-0 fz15">@Model.SingleProp.Floors</p>
                                            </div>
                                        </div>
                                    </div>
                                }
                                @if (Model.SingleProp.ConstructionYear != null)
                                {
                                    <div class="col-sm-6 col-lg-4">
                                        <div class="overview-element mb25 d-flex align-items-center">
                                            <span class="icon flaticon-event"></span>
                                            <div class="ml15">
                                                <h6 class="mb-0">Anul Construcției</h6>
                                                <p class="text mb-0 fz15">@Model.SingleProp.ConstructionYear</p>
                                            </div>
                                        </div>
                                    </div>
                                }
                            </div>
                        </div>
                        <div class="ps-widget mb30 overflow-hidden position-relative">
                            <h4 class="title fz17 mb30">Descriere</h4>
                            <p class="text mb10" style="white-space: pre-line;">@Model.SingleProp.Description</p>
                            <h4 class="title fz17 mb30 mt50">Detalii</h4>
                            <div class="row">
                                <div class="col-md-6 col-xl-4">
                                    <div class="d-flex justify-content-between">
                                        <div class="pd-list">
                                            @if (Model.SingleProp.Category != null)
                                            {
                                                <p class="fw600 mb10 ff-heading dark-color">Categorie</p>
                                            }
                                            @if (Model.SingleProp.Surface != null)
                                            {
                                                <p class="fw600 mb10 ff-heading dark-color">Suprafață</p>
                                            }
                                            @if (Model.SingleProp.Floors != null)
                                            {
                                                <p class="fw600 mb10 ff-heading dark-color">Etaje</p>
                                            }
                                            @if (Model.SingleProp.Rooms != null)
                                            {
                                                <p class="fw600 mb10 ff-heading dark-color">Camere</p>
                                            }
                                            @if (Model.SingleProp.Bathrooms != null)
                                            {
                                                <p class="fw600 mb-0 ff-heading dark-color">Băi</p>
                                            }

                                        </div>
                                        <div class="pd-list">
                                            @if (Model.SingleProp.Category != null)
                                            {
                                                <p class="text mb10">@Model.SingleProp.Category</p>
                                            }

                                            @if (Model.SingleProp.Surface != null)
                                            {
                                                <p class="text mb10">@Model.SingleProp.Surface m²</p>
                                            }
                                            @if (Model.SingleProp.Floors != null)
                                            {
                                                <p class="text mb10">@Model.SingleProp.Floors</p>
                                            }
                                            @if (Model.SingleProp.Rooms != null)
                                            {
                                                <p class="text mb10">@Model.SingleProp.Rooms</p>
                                            }
                                            @if (Model.SingleProp.Bathrooms != null)
                                            {
                                                <p class="text mb-0">@Model.SingleProp.Bathrooms</p>
                                            }
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-xl-4 offset-xl-2">
                                    <div class="d-flex justify-content-between">
                                        <div class="pd-list">
                                            @if (Model.SingleProp.GardenSurface != null)
                                            {
                                                <p class="fw600 mb10 ff-heading dark-color">Suprafața grădinii</p>
                                            }
                                            @if (Model.SingleProp.ConstructionYear != null)
                                            {
                                                <p class="fw600 mb10 ff-heading dark-color">Anul construcției</p>
                                            }
                                            <p class="fw600 mb10 ff-heading dark-color">Preț</p>
                                            <p class="fw600 mb10 ff-heading dark-color">Oportunitate investiție</p>
                                        </div>
                                        <div class="pd-list">
                                            @if (Model.SingleProp.GardenSurface != null)
                                            {
                                                <p class="text mb10">@Model.SingleProp.GardenSurface</p>
                                            }
                                            @if (Model.SingleProp.ConstructionYear != null)
                                            {
                                                <p class="text mb10">@Model.SingleProp.ConstructionYear</p>
                                            }
                                            <p class="text mb10">@Model.SingleProp.Price €</p>
                                            <p class="text mb10">@Model.SingleProp.IsInvestment</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="ps-widget mb30 overflow-hidden position-relative">
                            <h4 class="title fz17 mb30 mt30">Addresa</h4>
                            <div class="row">
                                <div class="col-md-6 col-xl-4">
                                    <div class="d-flex justify-content-between">
                                        <div class="pd-list">
                                            <p class="fw600 mb10 ff-heading dark-color">Address</p>
                                            <p class="fw600 mb10 ff-heading dark-color">City</p>
                                        </div>
                                        <div class="pd-list">
                                            <p class="text mb10">@Model.SingleProp.Address</p>
                                            <p class="text mb10">@Model.SingleProp.City</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-xl-4 offset-xl-2">
                                    <div class="d-flex justify-content-between">
                                        <div class="pd-list">
                                            <p class="fw600 mb10 ff-heading dark-color">Zip/Cod Postal</p>
                                            <p class="fw600 mb-0 ff-heading dark-color">Country</p>
                                        </div>
                                        <div class="pd-list">
                                            <p class="text mb10">@Model.SingleProp.Zip</p>
                                            <p class="text mb-0">@Model.SingleProp.Country</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-12">
                                    <div id="mapForDetails" class="h550" loading="lazy"></div>
                                </div>
                                <p id="latitude" class="text mb10" hidden>@Model.SingleProp.Latitude </p>
                                <p id="longitude" class="text mb10" hidden>@Model.SingleProp.Longitude </p>
                            </div>
                        </div>
                        <div class="ps-widget mb30 overflow-hidden position-relative">
                            <h4 class="title fz17 mb30">Facilitati</h4>
                            <div class="row">
                                @if (!string.IsNullOrEmpty(Model.SingleProp.Features))
                                {
                                    var features = Model.SingleProp.Features.Split(',');
                                    var columnCount = (int)Math.Ceiling((double)features.Length / 3);

                                    @for (int i = 0; i < 3; i++)
                                    {
                                        <div class="col-sm-6 col-md-4">
                                            <div class="pd-list">
                                                @for (int j = i * columnCount; j < Math.Min((i + 1) * columnCount, features.Length); j++)
                                                {
                                                    <p class="text mb10">
                                                        <i class="fas fa-circle fz6 align-middle pe-2"></i>
                                                        @features[j].Trim()
                                                    </p>
                                                }
                                            </div>
                                        </div>
                                    }
                                }
                            </div>
                        </div>
                        <div class="ps-widget mb30 overflow-hidden position-relative">
                            <h4 class="title fz17 mb30">Ce se afla in imprejurimi?</h4>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="navtab-style1">
                                        <div class="tab-content" id="nav-tabContent">
                                            <div class="tab-pane fade fz15 active show" id="nav-item1" role="tabpanel" aria-labelledby="nav-item1-tab">
                                                <div class="nearby d-sm-flex align-items-center mb20">
                                                    <div class="details">
                                                        <p class="dark-color fw600 mb-0">@Model.SingleProp.Nearby1</p>
                                                        <div class="blog-single-review">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="nearby d-sm-flex align-items-center mb20">
                                                    <div class="details">
                                                        <p class="dark-color fw600 mb-0">@Model.SingleProp.Nearby2</p>
                                                        <div class="blog-single-review">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="nearby d-sm-flex align-items-center">
                                                    <div class="details">
                                                        <p class="dark-color fw600 mb-0">@Model.SingleProp.Nearby3</p>
                                                        <div class="blog-single-review">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4">
                        <div class="column">
                            <div class="agen-personal-info position-relative bgc-white default-box-shadow1 bdrs12 p30 mt30" style="background:#FDFFEB; box-shadow: rgba(149, 157, 165, 0.2) 0px 8px 24px;">
                                <div class="widget-wrapper mb-0">
                                    <h6 class="title fz17 mb30">Află mai multe</h6>
                                    <div class="agent-single d-sm-flex align-items-center pb25">
                                        <div class="single-img mb30-sm">
                                            <img class="w90" src="~/images/team/neuro.png" alt="">
                                        </div>
                                        <div class="single-contant ml20 ml0-xs">
                                            <h6 class="title mb-1">Alex</h6>
                                            <div class="agent-meta mb10 d-md-flex align-items-center">
                                                <a class="text fz15" href="tel:+4366565667000"><i class="flaticon-call pe-1"></i>+43 665 65 66 7000</a>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="d-grid">
                                        <button onclick="makePhoneCall()" style="background: #FDFFEB" class="ud-btn btn-white2">Contactează<i class="fal fa-arrow-right-long"></i></button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row mt30 wow fadeInUp" data-wow-delay="700ms">
                    <div class="col-lg-9">
                        <div class="main-title2">
                            <h2 class="title">Alte Proprietati</h2>
                        </div>
                    </div>
                </div>
                <div class="row wow fadeInUp" data-wow-delay="900ms">
                    <div class="col-lg-12">
                        <div class="property-city-slider navi_pagi_top_right slider-dib-sm slider-3-grid owl-theme owl-carousel">
                            @foreach (var prop in Model.PropList)
                            {
                                <div class="item">
                                    <div class="listing-style1">
                                        <a asp-action="Details" asp-controller="Property" asp-route-id="@prop.Id" asp-route-language="@Context.Request.Query["language"]">
                                            <div class="list-thumb bdrs12">
                                                @if (!string.IsNullOrEmpty(prop.PhotoGuids))
                                                {
                                                    var photoNames = prop.PhotoGuids.Split(new[] { ',' }, StringSplitOptions.RemoveEmptyEntries);
                                                    var firstPhoto = photoNames.FirstOrDefault();

                                                    <img class="w-100" src="@Url.Action("GetImagesDetails", "Property", new { key = firstPhoto })" alt="">
                                                }
                                                else
                                                {
                                                    <img class="w-100" src="~/images/listings/g1-1.jpg" alt="">

                                                }
                                                @if (prop.IsFeatured == "Da")
                                                {
                                                    <div class="list-tag fz12"><span class="flaticon-electricity me-2"></span>RECOMANDAT</div>
                                                }
                                                <div class="list-price">@prop.Price €</div>
                                            </div>
                                        </a>
                                        <div class="list-content">
                                            <h6 class="list-title"><a asp-action="Details" asp-controller="Property" asp-route-id="@prop.Id" asp-route-language="@Context.Request.Query["language"]">@prop.Title</a></h6>
                                            <p class="list-text">@prop.City , @prop.Country</p>
                                            <div class="list-meta d-flex align-items-center">
                                                <a asp-action="Details" asp-controller="Property" asp-route-id="@prop.Id" asp-route-language="@Context.Request.Query["language"]"><span class="flaticon-home"></span>@prop.Category</a>
                                                @if (prop.Surface != null)
                                                {
                                                    <a asp-action="Details" asp-controller="Property" asp-route-id="@prop.Id" asp-route-language="@Context.Request.Query["language"]"><span class="flaticon-expand"></span>@prop.Surface m²</a>
                                                }
