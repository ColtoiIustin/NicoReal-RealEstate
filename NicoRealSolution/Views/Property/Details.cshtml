            minwidth: 0
        });
        // bind to scroll and resize events
        scrollbalance.bind();
    });

    $(document).ready(function () {

        if ($(window).width() > 768) {

            var containerWidth = $("#imageContainer").width();
            var containerHeight = $("#imageContainer").height();

            $('#imageContainerParent img').each(function () {
                var img = $(this);

                // Function to resize the image and fit it within the container
                function resizeImage(img) {
                    var imgWidth = img.width();
                    var imgHeight = img.height();
                    var widthRatio = containerWidth / imgWidth;
                    var heightRatio = containerHeight / imgHeight;
                    var scaleRatio = Math.min(widthRatio, heightRatio);

                    img.css({
                        width: imgWidth * scaleRatio,
                        height: imgHeight * scaleRatio
                    });
                }
                resizeImage(img);
            });
        }

        if ($(window).width() <= 768) {

